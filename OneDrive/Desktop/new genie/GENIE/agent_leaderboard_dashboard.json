{
    "title": "GENIE MARL Agent Leaderboard",
    "panels": [
        {
            "title": "Agent Performance Table",
            "type": "table",
            "targets": [
                {
                    "expr": "SELECT * FROM agent_leaderboard ORDER BY sharpe_ratio DESC"
                }
            ],
            "columns": [
                {
                    "text": "Agent",
                    "value": "agent_name"
                },
                {
                    "text": "Trades",
                    "value": "trades_executed"
                },
                {
                    "text": "Win Rate",
                    "value": "win_rate"
                },
                {
                    "text": "Avg PnL",
                    "value": "avg_pnl"
                },
                {
                    "text": "Drawdown",
                    "value": "max_drawdown"
                },
                {
                    "text": "Sharpe",
                    "value": "sharpe_ratio"
                }
            ]
        }
    ],
    "editable": true,
    "schemaVersion": 16
}